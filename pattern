void setup()
{
  size(600,600);
  
  }
void draw()
{
  background(0);
 
  
for(int b=0;b<10;b+=1)
{
  for (int c=0;c<100;c+=5)
  {
    for(int x=100;x<300;x+=15)
    {
      for(int y=30;y<360;y+=30){
       translate(350, 100);//change origin
       rotate(radians(y));
       strokeWeight(b);
       fill(c+50,150-c,255-c,100-c);
       triangle(0,0-x,0+x,0-x,0-x,0+x);}
    }
    }
  }
}
